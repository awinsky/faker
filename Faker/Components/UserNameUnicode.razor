@using Faker.Model
@inherits BaseFieldComponent
@if (Field != null)
{
      <FieldControl Field="Field" RemoveRequested="RemoveField"></FieldControl>

    if (UserNameUnicodeField != null)
    {
        <MatTextField @bind-Value="@UserNameUnicodeField.FirstName" Label="First Name"></MatTextField>
        <MatTextField @bind-Value="@UserNameUnicodeField.LastName" Label="Last Name"></MatTextField>

    }
}

@code {

    private UserNameUnicodeField? UserNameUnicodeField => Field?.Field as UserNameUnicodeField;

}