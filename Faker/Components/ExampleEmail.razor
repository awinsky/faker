@using Faker.Model
@inherits BaseFieldComponent
@if (Field != null)
{
    <FieldControl Field="Field" RemoveRequested="RemoveField"></FieldControl>

    if (ExampleEmailField != null)
    {
        <MatTextField @bind-Value="@ExampleEmailField.FirstName" Label="First Name"></MatTextField>
        <MatTextField @bind-Value="@ExampleEmailField.LastName" Label="Last Name"></MatTextField>
             
    }
}
@code {

    private ExampleEmailField? ExampleEmailField => Field?.Field as ExampleEmailField;

}